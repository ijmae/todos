<div class="card border rounded bg-info">
    <input type="text" maxlength="20" class="subject bg-transparent mt-2 font-weight-bold text-white text-center"
        value="<%= jobGroupName %>">
    <ul class="list-group m-3 bg-light">
        <!-- jobs -->
        <% jobs.forEach(function(job) { %>
        <%- include('load_job',{jobName: job.name, jobId: job._id, isDone: job.isDone, jobGroupId:jobGroupId}) %>
        <% }); %>
        <!-- jobs -->
    </ul>
    <div class="input-group mb-3 pl-3 pr-3">
        <input maxlength="20" type="text" class="form-control iptNewJob" placeholder="Nhập tên công việc"
            aria-label="Nhập tên nhóm" aria-describedby="basic-addon2">
        <button class="btn btn-success btnNewJob" type="button" data-jobGroupId=<%= jobGroupId %>>Tạo mới</button>
    </div>
</div>